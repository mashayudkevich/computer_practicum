int: N;
int: capacity;
array[1..N] of int: profit;
array[1..N] of int: size;
array[1..N] of var 0..1: x;
constraint forall(i in 1..N)(x[i] >= 0);
constraint forall(i in 1..N)(x[i] <=1);
constraint sum(i in 1..N)(size[i] * x[i]) <= capacity;
var int: sol = sum(i in 1..N)(profit[i] * x[i]);
solve maximize sol;
output [show(total) ++ "\n"];
